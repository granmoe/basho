  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-plugin-styled-components": "^1.3.0",
    "eslint": "^4.12.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
